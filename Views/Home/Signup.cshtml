                           @using (Html.BeginForm("Signup1", "Home", FormMethod.Post, new { id = "contact-form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

                            <div id="d2"><p style="color:red ; font-size:medium" ><b>@ViewBag.message</b></p></div>
                            <p>
                                   @* <input name="name" type="text" id="name" placeholder="User Name">*@
                                     @Html.TextBoxFor(model => model.Username , new { placeholder = "User Name" } )
                                    &nbsp&nbsp&nbsp&nbsp @Html.ValidationMessageFor(model => model.Username)
                                </p>
                                
                                <p>
                                   @Html.PasswordFor(model => model.Password, new { placeholder = "Your Password" })
                                    &nbsp&nbsp&nbsp&nbsp @Html.ValidationMessageFor(model => model.Password)
                                </p>
                
                    <input type="submit" class="mainBtn" id="submit" value="SignUp">
            }
            @section Scripts {
                @Scripts.Render("~/bundles/jqueryval")
            }
